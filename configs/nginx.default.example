server {
        listen 80;
        index index.html index.htm index.nginx-debian.html;

        server_name etfx.masterpro.site;

        location / {
                proxy_pass http://127.0.0.1:8082;
        }

        location /api {
                proxy_pass http://etfx.masterpro.site:1011/api;
        }

}
